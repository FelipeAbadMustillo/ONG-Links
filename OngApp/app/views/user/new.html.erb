<h1> Registrar usuario </h1>

<%= form_for @user do |f| %>
  <%= f.text_field :nombreUsu, placeholder: "Nombre" %>
  <%= f.text_field :apellidoUsu, placeholder: "Apellido" %>>
  <%= f.password_field :pwdUsu, placeholder: "ContraseÃ±a" %>
  <%= f.number_field :edad, in: 13.0..100.0, step:1, placeholder: "Edad" %>
  <%= f.text_field :ocupacion, placeholder: "Ocupacion" %>
  <%= f.email_field :mail, placeholder: "Direccion de Mail" %>
  <%= f.text_field :localidadUsu, placeholder: "Localidad Ej: C.A.B.A" %>
  <%= f.telephone_field :telUsu, placeholder: "Telefono" %>
  <%= f.sumbit "Registarse" %>
<% end %>
