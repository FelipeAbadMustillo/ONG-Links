<h1 class="login">Iniciar sesión: Usuario</h1>
<%= form_for :session do |f| %>

    <%= f.text_field :email, type: 'email' %>

    <%= f.password_field :password %>

    <%= f.submit "Iniciar Sesión" %>
<% end %>
<%= link_to "No tenés una cuenta? Registrate", sign_up_path %>
<%= link_to "Eres una Ong?", ong_sign_in_path %>
