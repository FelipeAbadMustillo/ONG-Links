<h1 class="login">Iniciar Sesión: Organización</h1>
<%= form_for :ong_session do |f| %>
  <%= f.text_field :email, type: 'email' %>
  <%= f.password_field :password %>
  <%= f.submit "Log In" %>
<% end %>
<span>No tienes una cuenta? Registra tu Ong <%= link_to "Aquí", ong_sign_up_path %></span>
<%= link_to "Eres un Usuario?", sign_in_path %>
